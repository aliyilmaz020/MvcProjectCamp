@model EntityLayer.Concrete.Admin
@{
    ViewBag.Title = "EditAdmin";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<script src="~/Templates/webadmin/plugins/jquery/jquery.min.js"></script>

<title>Kategori Güncelleme</title>
<form method="post">
    <div class="form-group-lg">
        @Html.HiddenFor(x => x.AdminId)
        @Html.LabelFor(x => x.AdminUserName,"Kullanıcı Adı")
        @Html.TextBoxFor(x => x.AdminUserName, new { @class = "form-control" })
        <br />
        @Html.LabelFor(x => x.AdminPassword,"Şifre")
        @Html.TextBoxFor(x => x.AdminPassword, new { @class = "form-control" })
        <br />
        @Html.LabelFor(x=>x.AdminRole,"Rol")
        @Html.TextBoxFor(x=>x.AdminRole, new { @class = "form-control" })
        <br />
        <button type="submit" class="btn btn-outline-success">Güncelle</button>
    </div>
</form>


